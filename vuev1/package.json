  "description": "用Vue.js编写的OMS基础架构",
  "main": "index.html",
  "scripts": {
    "start": "npm run dist && npm run server && npm run lanBuild && npm run plugin && npm run check",
    "dist": "cross-env NODE_ENV=development gulp",
    "build": "cross-env NODE_ENV=production gulp",
    "lanBuild": "gulp lanBuild && gulp jqueryPlugin",
    "check": "gulp watch",
    "plugin": "gulp jqueryPlugin",
    "scanComponents": "gulp scanComponents",
    "scanApi": "gulp scanApi",
    "stop": "pm2 delete all",
    "reboot": "npm run stop && npm run start",
    "server": "pm2 start server.js"
  },
